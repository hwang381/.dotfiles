# float / unfloat window and center on screen
shift + cmd - f : yabai -m window --toggle float;\
                  yabai -m window --grid 4:4:1:1:2:2

# relaunch
shift + cmd - x : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"
shift + cmd - z : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.skhd"

